{
  "name": "next-boilerplate",
  "version": "1.0.0",
  "description": "Portfolio",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaveBitter/portfolio.git"
  },
  "keywords": [
    "timer",
    "react",
    "next",
    "typescript",
    "graphql"
  ],
  "author": "Dave Bitter <daveybitter@gmail.com> (https://www.davebitter.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DaveBitter/next-boilerplate/issues"
  },
  "homepage": "https://github.com/DaveBitter/next-boilerplate#readme",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "deploy": "npm run build && npm run export",
    "storybook": "start-storybook -p 1337",
    "storybook:build": "build-storybook -c .storybook -o .storybook-build",
    "test": "jest --config config/tests/jest.config.js --verbose",
    "test:watch": "jest --config config/tests/jest.config.js --watch --verbose",
    "test:coverage": "jest --config config/tests/jest.config.js --coverage --verbose",
    "eslint": "eslint --fix",
    "stylelint": "stylelint **/*.scss --syntax=scss"
  },
  "lint-staged": {
    "*.js": "yarn eslint",
    "*.scss": "yarn stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "next": "9.3.5",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/addon-a11y": "^5.3.18",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.18",
    "@svgr/webpack": "^5.3.1",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/highlight.js": "^9.12.4",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@zeit/next-sass": "^1.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.4.0",
    "babel-loader": "^8.1.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^3.5.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "jest": "^25.4.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^10.1.7",
    "node-sass": "^4.13.1",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "storybook": "^5.3.18",
    "style-loader": "^1.1.4",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-strict-value": "^1.2.1",
    "stylelint-scss": "^3.17.1",
    "svg-react-loader": "^0.4.6",
    "ts-jest": "^25.4.0",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0"
  }
}